{
	"name": "@pyke/vibe",
	"version": "0.4.0",
	"publishConfig": {
		"access": "public"
	},
	"main": "index.node",
	"types": "index.d.ts",
	"license": "Apache-2.0",
	"description": "windows acrylic effects for electron ðŸ’«",
	"keywords": [
		"electron",
		"mica",
		"acrylic",
		"windows"
	],
	"author": "pyke.io",
	"repository": "github:pykeio/vibe",
	"bugs": "https://github.com/pykeio/vibe/issues",
	"homepage": "https://github.com/pykeio/vibe#readme",
	"scripts": {
		"build": "cargo-cp-artifact -nc index.node -- cargo build --message-format=json-render-diagnostics",
		"build:debug": "npm run build --",
		"build:release": "npm run build -- --release",
		"build:windows-i686": "npm run build -- --release --target=i686-pc-windows-msvc",
		"prebuild": "prebuildify --napi --tag-armv",
		"prepack": "prebuildify-ci download && ([ $(ls prebuilds | wc -l) = '2' ] || (echo 'Some prebuilds are missing'; exit 1))",
		"install": "npm run build:release && cargo clean",
		"test": "cargo test"
	},
	"dependencies": {
		"cargo-cp-artifact": "^0.1.6"
	},
	"peerDependencies": {
		"electron": ">=11.0"
	},
	"devDependencies": {
		"prebuildify": "^5.0.1",
		"prebuildify-ci": "^1.0.5"
	}
}
